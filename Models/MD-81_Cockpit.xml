<PropertyList>

<path>mesh_cockpit.ac</path>


<!-- Sub-Model Imports -->

<model>
  <name>OverheadPanel</name>
  <path>Aircraft/MD-80/Models/MD-81_Overhead.xml</path>
</model>

<model>
  <name>Instruments</name>
  <path>Aircraft/MD-80/Models/MD-81_Instruments.xml</path>
</model>


<!-- Hotspot Panels -->

<model>
  <name>2Dhotspots</name>
  <path>Aircraft/MD-80/Models/MD-81_Panels.xml</path>
</model>


<!-- Level Of Detail -->

<animation>
  <type>range</type>
  <object-name>Instruments</object-name>
  <min-m>0</min-m>
  <max-m>5</max-m>
</animation>
<animation>
  <type>range</type>
  <object-name>OverheadPanel</object-name>
  <min-m>0</min-m>
  <max-m>5</max-m>
</animation>


<animation>
