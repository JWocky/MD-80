<animation>
  <type>rotate</type>
  <object-name>KnobABS</object-name>
  <property>controls/autobrakes/ABS-select</property>
  <interpolation>
    <entry><ind>0</ind><dep>  40</dep></entry>
    <entry><ind>1</ind><dep>   0</dep></entry>
    <entry><ind>2</ind><dep>-40</dep></entry>
    <entry><ind>3</ind><dep>-105</dep></entry>
    <entry><ind>4</ind><dep>-150</dep></entry>
  </interpolation>   
  <center>
    <x-m>3.058</x-m>
    <y-m>0.123</y-m>
    <z-m>0.550</z-m>
  </center>
  <axis>
    <x>0.259</x>
    <y>0</y>
    <z>0.966</z>
  </axis>
</animation>
<animation>
  <type>pick</type>
  <object-name>KnobABS</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-adjust</command>
      <property>controls/autobrakes/ABS-select</property>
      <step>1</step>
      <min>0</min>
      <max>4</max>
      <wrap>false</wrap>
    </binding>
  </action>
  <action>
    <button>1</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-adjust</command>
      <property>controls/autobrakes/ABS-select</property>
      <step>-1</step>
      <min>0</min>
      <max>4</max>
      <wrap>false</wrap>
    </binding>
  </action>
</animation>

<animation>
  <type>select</type>
  <object-name>LampABS</object-name>
  <condition>
    <or>
      <property>/systems/autobrakes/armed</property>
      <equals>
        <property>/controls/autobrakes/ABS-select</property>
        <value>1</value>
      </equals>
    </or>
  </condition>
</animation>
<animation>
  <type>select</type>
  <object-name>LampABSon</object-name>
  <condition>
    <and>
      <not><property>/systems/autobrakes/armed</property></not>
      <not-equals>
        <property>/controls/autobrakes/ABS-select</property>
        <value>1</value>
      </not-equals>
    </and>
  </condition>
</animation>


