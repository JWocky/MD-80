  <object-name>KnobABS</object-name>
  <object-name>LampABS</object-name>
  <object-name>LampABSon</object-name>
</animation>


<!-- Panel glows -->

<animation>
  <type>material</type>
  <object-name>PedestalABS</object-name>
  <object-name>PedestalDrumFace</object-name>
  <emission>
    <red>0.4</red>
    <green>0.2</green>
    <blue>0.1</blue>
    <factor-prop>/controls/lighting/panel-norm</factor-prop>
  </emission>
</animation>


<!-- Animations -->

<animation>
  <type>rotate</type>
  <object-name>LGLever</object-name>
  <property>controls/gear/gear-down</property>
  <interpolation>
    <entry><ind>0</ind><dep>90</dep></entry>
    <entry><ind>1</ind><dep> 0</dep></entry>
  </interpolation>   
  <center>
    <x-m>2.175</x-m>
    <y-m>0</y-m>
    <z-m>0.909</z-m>
  </center>
  <axis>
    <x>0</x>
    <y>-1</y>
    <z>0</z>
  </axis>
</animation> 
<animation>
  <type>pick</type>
  <object-name>LGLever</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-toggle</command>
      <property>/controls/gear/gear-down</property>
    </binding>
  </action>
</animation>

<animation>
  <type>rotate</type>
  <object-name>FlapLever</object-name>
  <property>controls/flight/flaps</property>
  <interpolation>
    <entry><ind>    0</ind><dep> 0</dep></entry>
    <entry><ind>0.001</ind><dep>15</dep></entry>
    <entry><ind>0.275</ind><dep>28</dep></entry>
    <entry><ind>0.375</ind><dep>34</dep></entry>
    <entry><ind>  0.7</ind><dep>47</dep></entry>
    <entry><ind>    1</ind><dep>62</dep></entry>
  </interpolation>   
  <center>
    <x-m>2.616</x-m>
    <y-m>0</y-m>
    <z-m>0.654</z-m>
  </center>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
</animation> 

<animation>
  <type>pick</type>
  <object-name>FlapLever</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>controls.flapsDown(1)</script>
    </binding>
    <mod-up>
    <binding>
      <command>nasal</command>
      <script>controls.flapsDown(0)</script>
    </binding>
    </mod-up>
  </action>
</animation>
<animation>
  <type>pick</type>
  <object-name>FlapLever</object-name>
  <action>
    <button>1</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>controls.flapsDown(-1)</script>
    </binding>
    <mod-up>
    <binding>
      <command>nasal</command>
      <script>controls.flapsDown(0)</script>
    </binding>
    </mod-up>
  </action>
</animation>

<animation>
  <type>rotate</type>
  <object-name>ThrottleL</object-name>
  <object-name>ThrottleRevL</object-name>
