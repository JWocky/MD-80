  <object-name>ThrottleL</object-name>
  <object-name>ThrottleRevL</object-name>
  <action>
    <button>1</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-adjust</command>
      <property>/controls/engines/engine[0]/throttle</property>
      <step>-0.05</step>
      <min>0</min>
      <max>1</max>
      <wrap>false</wrap>
